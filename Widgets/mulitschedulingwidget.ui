      <layout class="QVBoxLayout" name="verticalLayout_72" stretch="0,1">
       <item>
        <layout class="QHBoxLayout" name="horizontalLayout_90">
         <item>
          <layout class="QGridLayout" name="gridLayout_4">
           <item row="1" column="2">
            <widget class="QLabel" name="label_chunkSize">
